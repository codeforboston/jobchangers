{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^7.2.1",
    "@types/d3": "^5.16.3",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.59",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-select": "^3.0.21",
    "@types/styled-components": "^5.1.3",
    "axios": "^0.21.1",
    "canvg": "^3.0.7",
    "d3": "^6.2.0",
    "jspdf": "^2.3.0",
    "lodash": "^4.17.20",
    "material-table": "^1.69.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "react-select": "^3.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.2.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:ci": "CI=1 react-scripts test --env=jest-environment-jsdom-sixteen --verbose --color --coverage --testPathIgnorePatterns DjangoApiClient",
    "test:all": "react-scripts test --env=jest-environment-jsdom-sixteen --watchAll --verbose",
    "eject": "react-scripts eject",
    "eslint": "eslint . --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "check-types": "tsc --noemit",
    "lint": "node scripts/npm.js lint",
    "ci:lint-and-test": "CI=true node scripts/npm.js lint-and-test",
    "fix": "node scripts/npm.js fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic",
    "chromatic:ci": "npx chromatic --exit-once-uploaded",
    "deploy": "firebase deploy --only hosting"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".*stories.tsx?"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.0",
    "@storybook/addon-actions": "^6.2.2",
    "@storybook/addon-essentials": "^6.2.2",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/node-logger": "^6.0.22",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.2.2",
    "@types/lodash": "^4.14.167",
    "babel-loader": "^8.1.0",
    "chromatic": "^5.7.1",
    "eslint": "^7.20.0",
    "eslint-config-react-app": "^6.0.0",
    "firebase-tools": "^9.8.0",
    "husky": "^4.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.4.0",
    "prettier": "2.1.2",
    "react-is": "^16.13.1",
    "react-select-event": "^5.1.0",
    "shelljs": "^0.8.4",
    "storybook-react-router": "^1.0.8",
    "ts-jest": "^26.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
